# JupyterHub secrets - generate secure values for production
# Generate with: openssl rand -hex 32

# Token for configurable-http-proxy authentication
CONFIGPROXY_AUTH_TOKEN=dev_secret_token_change_in_production

# Cookie secret file path (file is created automatically)
JUPYTERHUB_COOKIE_SECRET_FILE=/srv/jupyterhub/jupyterhub_cookie_secret

# Database URL (SQLite default)
# For PostgreSQL: postgresql://user:password@host:5432/jupyterhub
JUPYTERHUB_DB_URL=sqlite:///jupyterhub.sqlite
